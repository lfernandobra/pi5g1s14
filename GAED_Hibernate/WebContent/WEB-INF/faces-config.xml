<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>LoginBean</managed-bean-name>
  <managed-bean-class>edu.gaed.bean.LoginBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>UFBean</managed-bean-name>
  <managed-bean-class>edu.gaed.bean.UFBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>MunicipioBean</managed-bean-name>
  <managed-bean-class>edu.gaed.bean.MunicipioBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>LogradouroBean</managed-bean-name>
  <managed-bean-class>edu.gaed.bean.LogradouroBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>DisciplinaBean</managed-bean-name>
  <managed-bean-class>edu.gaed.bean.DisciplinaBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>ProfessorBean</managed-bean-name>
  <managed-bean-class>edu.gaed.bean.ProfessorBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>

<converter>
     <converter-id>converter.DisciplinaConverter</converter-id>
     <converter-class>edu.gaed.converter.DisciplinaConverter</converter-class>
</converter>
<converter>
     <converter-id>converter.AlunoConverter</converter-id>
     <converter-class>edu.gaed.converter.AlunoConverter</converter-class>
</converter>
 
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/home2.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/LoginModelo.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/home2.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/LoginModelo.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>secretaria</from-outcome>
   <to-view-id>/secretaria.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
