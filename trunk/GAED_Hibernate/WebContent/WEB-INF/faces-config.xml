<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<converter>
		<converter-id>converter.DisciplinaConverter</converter-id>
		<converter-class>edu.gaed.converter.DisciplinaConverter</converter-class>
	</converter>
	<converter>
		<converter-id>converter.AlunoConverter</converter-id>
		<converter-class>edu.gaed.converter.AlunoConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>LoginBean</managed-bean-name>
		<managed-bean-class>edu.gaed.bean.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/home.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>aluno</from-outcome>
			<to-view-id>/aluno.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>boletim</from-outcome>
			<to-view-id>/boletimalunodisciplina.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>config</from-outcome>
			<to-view-id>/configuracoes.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>gestor</from-outcome>
			<to-view-id>/gestor.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ocorrencia</from-outcome>
			<to-view-id>/ocorrencia.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>professor</from-outcome>
			<to-view-id>/professor.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>responsavel</from-outcome>
			<to-view-id>/responsavel.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>secretaria</from-outcome>
			<to-view-id>/secretaria.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>tarefa</from-outcome>
			<to-view-id>/tarefa.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>turma</from-outcome>
			<to-view-id>/turma.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/ocorrencia.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/tarefa.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/boletimalunodisciplina.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/aluno.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/gestor.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/professor.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/responsavel.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>boletim</from-outcome>
			<to-view-id>/boletimalunodisciplina.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/secretaria.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/turma.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/configuracoes.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application />
</faces-config>
